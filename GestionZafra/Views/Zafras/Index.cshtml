@model IEnumerable<GestionZafra.Models.Zafras>

@{
    ViewBag.Title = "Zafras";
    ViewBag.Message = "Zafras";
}
<div class="content">
    <div class="row-fluid">
        <div class="span12">
            <div class="block">
                <div class="head dblue">
                    
                </div>
                <div class="data-fluid">
                    <table class="table fpTable lcnp" cellpadding="0" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.descripcionZafra)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.fechaInicio)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.fechaFin)
                                </th>
                                <th width="80" class="TAC">Acciones</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.descripcionZafra)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.fechaInicio)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.fechaFin)
                                    </td>
                                    <td>
                                        <a href="@Url.Action("Edit","Zafras")/@item.id" class="button green">
                                            <div class="icon"><span class="ico-pencil"></span></div>
                                        </a>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>