@model GestionZafra.Models.Campo

@{
    ViewBag.Title = "Nuevo";
}
<div class="span5">
    @using (Html.BeginForm())
    {
        @Html.ValidationSummary(true)

        <fieldset class="data-fluid">
            <legend>Campo</legend>

            <div class="row-form">
                @Html.LabelFor(model => model.Suministradoresid, "Suministradores")
            </div>
            <div class="row-form">
                @Html.DropDownList("Suministradoresid")
                @Html.ValidationMessageFor(model => model.Suministradoresid)
            </div>

            <div class="row-form">
                @Html.LabelFor(model => model.cantCanaVerde)
            </div>
            <div class="row-form">
                @Html.EditorFor(model => model.cantCanaVerde)
                @Html.ValidationMessageFor(model => model.cantCanaVerde)
            </div>

            <div class="row-form">
                @Html.LabelFor(model => model.cantCanaQuemada)
            </div>
            <div class="row-form">
                @Html.EditorFor(model => model.cantCanaQuemada)
                @Html.ValidationMessageFor(model => model.cantCanaQuemada)
            </div>

            <div class="row-form">
                @Html.LabelFor(model => model.cepa)
            </div>
            <div class="row-form">
                @Html.EditorFor(model => model.cepa)
                @Html.ValidationMessageFor(model => model.cepa)
            </div>

            <div class="row-form">
                @Html.LabelFor(model => model.VariedadCanaid, "VariedadCana")
            </div>
            <div class="row-form">
                @Html.DropDownList("VariedadCanaid")
                @Html.ValidationMessageFor(model => model.VariedadCanaid)
            </div>
            <div class="toolbar bottom tar">
                @Html.ActionLink("Lista de Campos", "Index",null,new{Class="btn"})
                <input class="btn btn-success" type="submit" value="Nuevo" />
            </div>
        </fieldset>
    }
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
