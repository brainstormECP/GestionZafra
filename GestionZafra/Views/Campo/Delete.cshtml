@model GestionZafra.Models.Campo

@{
    ViewBag.Title = "Eliminar";
}
<h3>Esta seguro que desea eliminar este Campo?</h3>
<div class="span5">
    <fieldset class="data-fluid">
        <legend>Campo</legend>

        <div class="row-form">
            @Html.DisplayNameFor(model => model.Suministradores.nombreSuministrador):
        <div class="label label-warning">
            @Html.DisplayFor(model => model.Suministradores.nombreSuministrador)
        </div>
        </div>



        <div class="row-form">
            @Html.DisplayNameFor(model => model.cantCanaVerde):
        <div class="label label-warning">
            @Html.DisplayFor(model => model.cantCanaVerde)
        </div>
        </div>


        <div class="row-form">
            @Html.DisplayNameFor(model => model.cantCanaQuemada):
        <div class="label label-warning">
            @Html.DisplayFor(model => model.cantCanaQuemada)
        </div>
        </div>


        <div class="row-form">
            @Html.DisplayNameFor(model => model.cepa):
        <div class="label label-warning">
            @Html.DisplayFor(model => model.cepa)
        </div>
        </div>


        <div class="row-form">
            @Html.DisplayNameFor(model => model.VariedadCana.nombreVariedad):
        <div class="label label-warning">
            @Html.DisplayFor(model => model.VariedadCana.nombreVariedad)
        </div>
        </div>

    </fieldset>
    @using (Html.BeginForm())
    {
        <div class="toolbar bottom tar">
            @Html.ActionLink("Lista de Campos", "Index",null,new{Class="btn"})
            <input class="btn btn-danger" type="submit" value="Eliminar" />
        </div>
    }
</div>
