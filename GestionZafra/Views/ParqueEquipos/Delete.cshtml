@model GestionZafra.Models.ParqueEquipos

@{
    ViewBag.Title = "Eliminar";
}
<h3>Esta seguro que desea eliminar este Parque de Equipos?</h3>
<div class="span5">
    <fieldset class="data-fluid">
        <legend>Parque de Equipos</legend>

        <div class="row-form">
            @Html.DisplayNameFor(model => model.cantidadEquipos)
            <div class="label label-warning">
                @Html.DisplayFor(model => model.cantidadEquipos)
            </div>
        </div>


        <div class="row-form">
            @Html.DisplayNameFor(model => model.TipoEquipos.descripcionEquipo)
            <div class="label label-warning">
                @Html.DisplayFor(model => model.TipoEquipos.descripcionEquipo)
            </div>
        </div>


        <div class="row-form">
            @Html.DisplayNameFor(model => model.Suministradores.nombreSuministrador)
            <div class="label label-warning">
                @Html.DisplayFor(model => model.Suministradores.nombreSuministrador)
            </div>
        </div>

    </fieldset>
    @using (Html.BeginForm())
    {
        <div class="toolbar bottom tar">
            @Html.ActionLink("Lista de Parque de Equipos", "Index", null, new { Class = "btn" })
            <input class="btn btn-danger" type="submit" value="Eliminar" />
        </div>
    }
</div>
