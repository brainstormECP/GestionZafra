@model GestionZafra.Models.Suministradores

@{
    ViewBag.Title = "Editar";
}
<div class="span5">
    @using (Html.BeginForm())
    {
        @Html.ValidationSummary(true)

        <fieldset class="data-fluid">
            <legend>Suministradores</legend>

            @Html.HiddenFor(model => model.id)
            
            <div class="row-form">
                @Html.LabelFor(model => model.TiposSectorPropiedadid, "TiposSectorPropiedad")
            </div>
            <div class="row-form">
                @Html.DropDownList("TiposSectorPropiedadid")
                @Html.ValidationMessageFor(model => model.TiposSectorPropiedadid)
            </div>

            <div class="row-form">
                @Html.LabelFor(model => model.nombreSuministrador)
            </div>
            <div class="row-form">
                @Html.EditorFor(model => model.nombreSuministrador)
                @Html.ValidationMessageFor(model => model.nombreSuministrador)
            </div>
            
             <div class="row-form">
                @Html.LabelFor(model => model.activo)
            </div>
            <div class="row-form">
                @Html.EditorFor(model => model.activo)
                @Html.ValidationMessageFor(model => model.activo)
            </div>

            <div class="toolbar bottom tar">
                 @Html.ActionLink("Atras", "Index",null,new{Class="btn"})
                <input class="btn btn-success" type="submit" value="Guardar" />
            </div>
        </fieldset>
    }
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
